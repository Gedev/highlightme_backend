{
  "builds": [
    {
      "services": [
        "web"
      ],
      "commands": [
        "npm install",
        "npm run build",
        "python manage.py migrate"
      ]
    },
    {
      "services": [
        "discordbot"
      ]
    }
  ],
  "deploys": [
    {
      "services": [
        "web"
      ],
      "commands": [
        "python manage.py migrate",
        "gunicorn highlightme.wsgi"
      ]
    },
    {
      "services": [
        "discordbot"
      ],
      "commands": [
        "python manage.py runbot"
      ]
    }
  ]
}
